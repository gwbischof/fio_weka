# report bandwidth iops latency
[detector-readwrite]
description='detector read/write workload'
ioengine=posixaio
iodepth=1
create_on_open=1
unlink=1
blocksize=1m
filesize=5m-15m
nrfiles=1000
startdelay=1
time_based=1
runtime=60
numjobs=90
group_reporting=1
direct=1
ramp_time=5
rw=randrw
openfiles=1
